{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/StrategyParams",
  "definitions": {
    "StrategyParams": {
      "type": "object",
      "properties": {
        "env": {
          "type": "string",
          "enum": ["livenet", "testnet"]
        },
        "space": {
          "type": "string"
        },
        "proposal": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": ["fixed", "ratio"]
        },
        "amount": {
          "type": "string"
        },
        "min": {
          "type": "string"
        },
        "max": {
          "type": "string"
        }
      },
      "required": ["type", "amount"],
      "additionalProperties": false
    }
  }
}
